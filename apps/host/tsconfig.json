{
    "compilerOptions": {
        "target": "es2016",
        "module": "commonjs",
        "types": ["webpack-env", "jest", "node"],
        "baseUrl": "./src",
        "paths": {
            "@assets/*": ["src/assets/*"],
            "@components/*": ["src/components/*"],
            "@presets/*": ["src/primePreset/*"],
            "@router/*": ["src/router/*"],
            "@types/*": ["src/interfaces/*"]
        },
        "lib": ["esnext", "dom", "dom.iterable", "scripthost"],
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
    },
    "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.vue", "src/**/*.tsx", "shims-vue.d.ts"],
    "exclude": ["node_modules"]
}
